{
  "name": "audiobooks-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Alexey Butkeev, Egor Filin",
  "scripts": {
    "dev": "vite",
    "prestart": "npm run prebuild",
    "start": "vite --open",
    "prebuild": "npm run compile-openapi",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format:check": "prettier -c .",
    "format": "prettier -w .",
    "compile-openapi": "rtk-query-codegen-openapi src/api/openapi-config.ts",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg",
    "i18next:extract": "npx i18next-parser --config ./src/locales/i18next-parser.config.ts",
    "preview": "npm run build && vite preview --open"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@passwordless-id/webauthn": "^1.2.6",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.1",
    "copy-to-clipboard": "^3.3.3",
    "cyrillic-to-translit-js": "^3.2.1",
    "generate-password-browser": "^1.1.0",
    "i18next": "^23.7.3",
    "i18next-browser-languagedetector": "^7.2.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^13.4.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "socket.io-client": "^4.7.2",
    "throttle-debounce": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@types/debounce": "^1.2.3",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "i18next-parser": "^9.3.0",
    "prettier": "^3.0.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
